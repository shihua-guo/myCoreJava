package com.briup.java_day14.course;

public enum EnumRGB {
	RED(255,0,0),GREEN(0,255,0),BLUE(0,0,255);
	private EnumRGB(){}
	private EnumRGB(int r,int g,int b){
		System.out.println("RGB:"+r+","+g+","+b);
	}
	public static void main(String[] args) {
		EnumRGB rgb = EnumRGB.RED;
		rgb = EnumRGB.GREEN;
		rgb = EnumRGB.BLUE;
	}
}
